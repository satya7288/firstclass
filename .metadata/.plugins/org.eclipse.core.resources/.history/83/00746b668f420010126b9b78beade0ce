package db_connect;

import java.sql.Connection;
import java.sql.DriverManager;

import com.mysql.jdbc.PreparedStatement;

public class db_connection {

	public static void main(String[] args) {
		int id;
		String name;
		String email;
		
		try {
		Class.forName("com.mysql.jdbc.Driver");
	Connection con=	DriverManager.getConnection("jdbc:mysql://localhost:3306/aits","root","root");
		System.out.println(con);
	java.sql.PreparedStatement pst=	con.prepareStatement("insert into stdemo values(109,'venkat','venkat@gmail.com')");
		int i=pst.executeUpdate();
		System.out.println("sucess"+i);
		}catch(Exception e) {
			e.printStackTrace();
		}
		}
}
